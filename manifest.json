{
  "manifest_version": 2,
  "name": "Sync Chrome Bookmarks",
  "version": "0.1",
  "homepage_url": "https://github.com/taroxd/firefox-extension-sync-chrome-bookmarks",
  "description": "Remove all bookmarks and then import bookmarks from chrome",

  "permissions": [
    "bookmarks"
  ],

  "background": {
    "scripts": ["Bookmarks.js", "background.js"]
  }
}
